<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Regression Denmark Carwash">
    <test name="Denmark Carwash">
        <groups>
            <run>
                <include name="denmark.carwash"/>
            </run>
        </groups>
        <classes>
            <class name="carwash.PostNewCarwashOrder" />
            <class name="carwash.CheckCarwashStatusCreated" />
            <class name="carwash.MobilePayReserveFundsCarwash" />
            <class name="carwash.CheckCarwashStatusReserved" />
            <class name="carwash.CheckCarwashStatusWaitingForCustomer" />
            <class name="carwash.AuthorizeCarwash" />
            <class name="carwash.CheckCarwashStatusServiceReady" />
            <class name="carwash.CheckCarwashStatusServiceInUse" />
            <class name="carwash.CheckCarwashStatusServiceCompleted" />
        </classes>
    </test>
</suite>
