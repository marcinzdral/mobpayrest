<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Regression Denmark Fuel">
    <test name="Denmark Fuel">
        <groups>
            <run>
                <include name="denmark.fuel" />
            </run>
        </groups>
        <classes>
            <class name="fuel.PostNewFuelOrder" />
            <class name="fuel.CheckFuelingStatusCreated" />
            <class name="fuel.MobilePayReserveFundsFuel" />
            <class name="fuel.CheckFuelingStatusReserved" />
            <class name="fuel.VerifyReservation" />
            <class name="fuel.UnlockNozzle" />
            <class name="fuel.CheckFuelingStatusServiceReady" />
            <class name="fuel.CheckFuelingStatusServiceFinanced" />
        </classes>
    </test>
</suite>